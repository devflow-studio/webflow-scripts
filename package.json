{
  "name": "@devflow-studio/webflow-scripts",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{css,html,js,ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{css,html,ts,tsx,md}\"",
    "prepare": "husky install",
    "test": "turbo test",
    "types:check": "turbo types:check"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "turbo": "^2.3.4",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
